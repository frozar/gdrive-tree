{
  "private": true,
  "engines": {
    "node": ">=4"
  },
  "devDependencies": {
    "parcel": "^2.2.1"
  },
  "scripts": {
    "dev": "parcel -p 9000 app/index.html",
    "serve:test": "cross-env NODE_ENV=test gulp serve",
    "serve:dist": "cross-env NODE_ENV=production gulp serve",
    "start": "gulp serve",
    "build": "cross-env NODE_ENV=production gulp",
    "test": "npm run serve:test",
    "tasks": "gulp --tasks"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR"
  ],
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "rules": {}
  },
  "dependencies": {
    "@viselect/vanilla": "^3.0.0-beta.13",
    "html-escaper": "^3.0.3",
    "regenerator-runtime": "^0.13.9",
    "tabbable": "^5.2.1"
  }
}
